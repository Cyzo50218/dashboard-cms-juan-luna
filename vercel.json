{
  "version": 2,
  "builds": [
    {
      "src": "./**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/$",
      "status": 307,
      "headers": { "Location": "/login/" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}