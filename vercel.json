{
  "version": 2,
  "builds": [
    {
      "src": "./**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/$",
      "status": 307,
      "headers": {
        "Location": "/login/"
      }
    },
    {
      "src": "/register/(.*)",
      "dest": "/registration/$1"
    },
    {
      "src": "/invitation/([^/]+)",
      "dest": "/registration/register.html"
    },
    {
      "src": "/(.*\\.(?:js|css|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|otf))",
      "dest": "/$1"
    },
    {
      "src": "/mainpage/([^/]+)",
      "dest": "/landingpage/index.html"
    },
    {
      "src": "/workspace-invite/([^/]+)",
      "dest": "/registration/register.html"
    },
    {
      "src": "/workspace-invite/([^/]+)",
      "dest": "/registration/register.html"
    },
    {
      "src": "/logo.png",
      "dest": "/assets/logo.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}