{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/**/*",
      "use": "@vercel/static"
    },
    {
    "src": "*",
    "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dashboard/$1"
    },
     {
      "src": "/(.*)",
      "dest": "/$1"
    
  ]
}
