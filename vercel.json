{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/([^/]+)/?$",
      "dest": "/dashboard/$1/index.html"
    },
    {
      "src": "^/([^/]+)/(.+)$",
      "dest": "/dashboard/$1/$2"
    }
  ]
}
