{
  "version": 2,
  "builds": [
    {
      "src": "./**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/attachments/(.*)",
      "destination": "https://us-central1-juan-luna-db.cloudfunctions.net/attachments/$1"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/$",
      "status": 307,
      "headers": {
        "Location": "/login/"
      }
    },
    {
      "src": "/register/(.*)",
      "dest": "/registration/$1"
    },
    {
      "src": "/invitation/([^/]+)",
      "dest": "/registration/register.html"
    },
    {
      "src": "/workspace-invite/([^/]+)",
      "dest": "/registration/register.html"
    },
    {
      "src": "/workspace-invite/([^/]+)",
      "dest": "/registration/register.html"
    },
    {
      "src": "/logo.png",
      "dest": "/assets/logo.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}